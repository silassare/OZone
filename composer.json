{
	"name": "silassare/ozone",
	"version": "2.0.0",
	"description": "OZone is a framework to build RESTFul API and website.",
	"license": "MIT",
	"keywords": [
		"ozone",
		"api-generator",
		"ozone-framework",
		"framework",
		"php",
		"api",
		"website"
	],
	"require": {
		"php": ">=5.6.0",
		"ext-pdo": "*",
		"ext-json": "*",
		"ext-gd": "*",
		"ext-libxml": "*",
		"ext-simplexml": "*",
		"ext-posix": "*",
		"silassare/otpl": "dev-master",
		"silassare/gobl": "dev-master",
		"silassare/kli": "dev-master",
		"claviska/simpleimage": "^3.3",
		"psr/http-message": "^1.0"
	},
	"require-dev": {
		"friendsofphp/php-cs-fixer": "^2.16",
		"squizlabs/php_codesniffer": "^3.5",
		"phpcompatibility/php-compatibility": "*"
	},
	"scripts": {
		"post-install-cmd": "\"vendor/bin/phpcs\" --config-set installed_paths vendor/phpcompatibility/php-compatibility",
		"post-update-cmd": "\"vendor/bin/phpcs\" --config-set installed_paths vendor/phpcompatibility/php-compatibility"
	},
	"bin": [
		"bin/oz",
		"bin/ozone"
	],
	"homepage": "https://github.com/silassare/ozone#readme",
	"archive": {
		"exclude": [
			"/ignore"
		]
	},
	"autoload": {
		"psr-4": {
			"OZONE\\OZ\\": "oz"
		}
	},
	"authors": [
		{
			"name": "Emile Silas Sare",
			"email": "emile.silas@gmail.com",
			"homepage": "https://github.com/silassare"
		}
	],
	"minimum-stability": "dev",
	"prefer-stable": true
}
